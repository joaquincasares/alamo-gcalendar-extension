{
  "name": "(Unofficial) Alamo Showtime to gCalendar",
  "version": "1.0.0.3",
  "manifest_version": 2,

  "description": "Displays a link to quickly add Alamo Drafthouse showtimes to Google Calendar",
  "icons": { "16": "icon16.png",
             "48": "icon48.png",
             "128": "icon128.png" },

  "background": "background.html",
  "content_scripts": [
    {
      "matches": ["http://drafthouse.com/*"],
      "js": ["jquery-1.9.1.min.js", "date.format.js", "content.js"]
    }
  ]
}
